<div class="post-container">
  <div class="page-title">
    <h1>コメントしたアカウント</h1>
  </div><!-- /.page-title -->

  <ul>
    <% @comment_users.each do |comment_user| %>
      <div class="user-container">
        <div class="content-header">
          <%= link_to user_path(comment_user) do %>
            <p><%= image_tag 'default.png', alt: comment_user.name, class: 'cover50' %></p>
          <% end %>
            <div class="post-names">
              <p class="post-user"><%= link_to comment_user.name, user_path(comment_user) %></p>
            </div><!-- /.post-names -->
        </div><!-- /.content-header -->
      </div><!-- /.user-container -->
    <% end %>
  </ul>
</div><!-- /.like-container -->
