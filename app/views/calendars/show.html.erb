<div class="container mt-3">
  <h1>カレンダー</h1>

  <div class="mt-3">
    <%= month_calendar attribute: :created_at, events: @workouts do |date, workouts| %>
      <%= date.day %>

      <% workouts.each do |workout| %>
        <div class="calendar_exerdise_names">
          <%= link_to workout.exercise.name, workout_path(workout) %>
        </div>
      <% end %>
    <% end %>
  </div>
</div><!-- /.container -->
